    @include extjs-panel-ui(
        $ui: 'default'
    );
}

@if $include-panel-default-framed-ui {
    @include extjs-panel-ui(
        $ui: 'default-framed',

        $ui-border-color: $panel-frame-border-color,
        $ui-border-width: $panel-frame-border-width,
        $ui-border-radius: $panel-frame-border-radius,
